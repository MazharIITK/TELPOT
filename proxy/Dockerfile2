FROM ubuntu:latest

MAINTAINER mazhar <mkhan31995@gmail.com>
#MENTOR Rohit Sehgal, Nishit Mahendrabhai Majithia

RUN apt-get update 
RUN apt-get -y install -q build-essential && apt-get -y install -q python-dev libffi-dev libssl-dev python-pip
RUN pip install service_identity pycrypto && pip install twisted==14.0.0

copy proxy.py proxy.py

EXPOSE 9620

ENTRYPOINT ["python2.7"]
CMD ["proxy.py"]
